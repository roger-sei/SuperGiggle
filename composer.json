{
    "autoload": {
        "psr-4": {
            "SuperGiggle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SuperGiggle\\": "tests/src/"
        }
    },
    "name": "roger-sei/super-giggle",
    "description": "SuperGiggle checks the coding standards for a specific commit change",
    "require": {
        "squizlabs/php_codesniffer": "3.*",
        "php": ">=7.2"
    },
    "license": "MIT",
    "bin": [
        "bin/super-giggle"
    ],
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "scripts": {
        "test": "phpunit --testdox --colors=always"
    }
}
